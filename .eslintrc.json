{
    "extends": "airbnb",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "semi": 2,
         "react/jsx-uses-react": 1,
         "comma-dangle": [2, "always-multiline"]
    },
    "env": {
        "browser": true,
        "node": true
    }
}
